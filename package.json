{
  "name": "naitei2024_e-learning",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx ts-node build.ts",
    "start": "node dist/bin/www.js",
    "dev:start": "nodemon --exec ts-node src/bin/www.ts",
    "server:start": "SET DEBUG=naitei2024_e-learning:* & npm run dev:start",
    "watch": "tsc -w",
    "lint": "eslint src/**/*.{ts,tsx,json,pug}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx,json,pug}",
    "format": "prettier --write src/**/*.{ts,tsx,json,pug}",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "migrate:generate": "npm run typeorm -- migration:generate -d ./src/config/data-source.ts",
    "migrate:run": "npm run typeorm -- migration:run -d ./src/config/data-source.ts",
    "migrate:revert": "npm run typeorm -- migration:revert -d ./src/config/data-source.ts",
    "migrate:show": "npm run typeorm -- migration:show -d ./src/config/data-source.ts",
    "tailwind:css": "postcss src/public/stylesheets/tailwind.css -o src/public/stylesheets/global.css -w"
  },
  "dependencies": {
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.5",
    "dotenv": "^16.4.5",
    "express": "~4.19.2",
    "express-async-handler": "^1.2.0",
    "fs-extra": "^11.2.0",
    "http-errors": "~2.0.0",
    "i18next": "^23.12.2",
    "i18next-fs-backend": "^2.3.2",
    "i18next-http-middleware": "^3.6.0",
    "jet-logger": "^1.3.1",
    "morgan": "~1.10.0",
    "mysql2": "^3.10.3",
    "pug": "^3.0.3",
    "react": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.7.0",
    "@prettier/plugin-pug": "^3.0.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.8.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.40",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "description": "Project for learning Node.js Express tutorial.",
  "main": "src/bin/www.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awesome-academy/naitei2024_e-learning.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/awesome-academy/naitei2024_e-learning/issues"
  },
  "homepage": "https://github.com/awesome-academy/naitei2024_e-learning#readme"
}
